<ul>

	<% @users.each do |user| %>
	<li><%= link_to user.username, "/users/#{user.id}" %></li>
	<li><%= link_to "edit", edit_user_path(current_user) %></li>
	<li><%= link_to "delete", user, method: :delete,
                                  data: { confirm: "Are you sure?" } %><% end %> </li>

	<% end %>
	
</ul>
